pipeline {
   agent any

   tools {
      // Install the Maven version configured as "M3" and add it to the path.
      maven "M3"
   }

   stages {
      stage('Pull') {
         steps {
            // Get some code from a GitHub repository
            git 'https://github.com/anandsanjayreddy/crudApp.git'
         }
      }
      stage('Build') {
          steps {
            // Run Maven on a Unix agent.
            sh "mvn clean package"
         }
      }
   }
}
